/* this code computes the frequency f characters in a string
with o(n2) appraoch*/
public class stringFrequency{

     public static void main(String []args){
        System.out.println("Hello World");
        String s1 = new String("elephant");
        classdemo.checkInclusion(s1);
     }
        static class classdemo{
    static void checkInclusion(String s1) {
        int len = s1.length();
        System.out.print(len);
        char[] ch = new char[len];
        char temp;
        //char[] ch = new char[len];
        for(int j =0;j<len;j++)
        {
            int count=0;
            temp =s1.charAt(j);
        for(int i =0;i<len;i++)
        {
            
            
        if(s1.charAt(i)==temp)
        {
        count++;
        ch[i]=s1.charAt(i);
        }
        
        }
        System.out.println("countof "+ch[j]+"is"+count);
        }
        for(int i=0;i<len;i++)
        System.out.print(ch[i]);   
        }

     }
